		TNT1 A 0 A_Explode(random(160,250),256,0)
		TNT1 A 0 A_Jump(255,"T1","T2","T3","T4")
		Goto T1
	T1:
		DB42 A 0 A_SetScale(-1.35,1.35)
		Goto EndAnimation
	T2:
		DB42 A 0
		Goto EndAnimation
	T3:
		DB43 A 0 A_SetScale(-1.35,1.35)
		Goto EndAnimation
	T4:
		DB43 A 0
		Goto EndAnimation
	EndAnimation:
		"####" ABCDE 2 Bright
		"####" FGHIJK 2 Bright
		"####" LMNOPQRSTUVWXYZ 2 Bright A_FadeOut(0.066)
		Stop
	}
}



