	Altfire:
		TNT1 A 0 A_JumpIfInventory("WeaponZoomedToken", 1, "ZoomOut")
		TNT1 A 0 A_JumpIfInventory("RailgunFired", 1, "DoTheMathToZoom")
	ZoomIn:
		QCG7 A 3
		TNT1 A 0 A_ZoomFactor(3.5)
		TNT1 A 0 A_PlaySound("QC/Weapons/Railgun/ZoomIn", 7, 1.0, 0)
		TNT1 A 0 A_GiveInventory("WeaponZoomedToken",1)
		TNT1 A 0 A_JumpIfInventory("RG_UG_Explosive",1,"PlayUpgradeBeep")
		Goto ZoomInHeld
		
		PlayUpgradeBeep:
			TNT1 A 0 A_PlaySound("QC/Weapons/Upgrade/RGBeep",CHAN_AUTO)
			"####" "#" 0 ACS_NamedExecuteAlways("QCDE_ModSelectedText",0,19)
			Goto ZoomInHeld
			
	ZoomInHeld:
		R016 P 0 A_JumpIfInventory("NoWeaponFireToken",1,"ReadyCantFire")
		R016 P 0 A_JumpIfInventory("RailgunCharges",52,"ReadyZoomedCharged")
		R016 P 1 Bright A_GiveInventory("RailgunCharges",1)
		R016 P 1 Bright A_Refire("ZoomInHeld")
		Goto ReadyZoomed
		
		ZoomOut:
			TNT1 A 0 A_JumpIfInventory("RailgunFired", 1, "DoTheMathFromZoom")
			TNT1 A 0 A_ZoomFactor(1.0) 
			TNT1 A 0 A_TakeInventory("RailgunCharges",52)
			TNT1 A 0 
			TNT1 A 0 A_PlaySound("QC/Weapons/Railgun/ZoomOut", 7, 1.0, 0)
			TNT1 A 0 A_TakeInventory("WeaponZoomedToken",32767)
		ZoomOutHeld:
			QCG7 A 0 A_JumpIfInventory("NoWeaponFireToken",1,"ReadyCantFire")
			QCG7 A 1 
			QCG7 A 1 A_Refire("ZoomOutHeld")
			Goto ReadyAgain

